<project name="" version="1.0" author="" date="" description="" comment="" references="" working_directory="" temp_root="" run="1" file_root="" cache="yes" cleanup="yes">
  <data>
    <ccpn_model filename=""/>
    <molecule file="" format="xml">
      <linkage_definition name="automatic" filename=""/>
      <topology_definition name="automatic" filename=""/>
      <parameter_definition name="automatic" filename=""/>
    </molecule>
    <initial_structure file="" format="iupac" enabled="no"/>
    <symmetry enabled="no" n_monomers="1" symmetry_type="None" ncs_enabled="no" packing_enabled="no"/>
  </data>
  <structure_generation engine="cns">
    <cns keep_output="yes" create_psf_file="yes" generate_template="yes" local_executable="" nonbonded_parameters="PROLSQ">
      <annealing_parameters>
        <unambiguous_restraints first_iteration="0" k_hot="10.0" k_cool2="50.0" k_cool1_initial="10.0" k_cool1_final="50.0"/>
